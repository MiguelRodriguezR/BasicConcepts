        android:text="="
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonOpenPatentesis"
        app:layout_constraintEnd_toEndOf="@+id/textViewScreen"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/buttonOpenPatentesis"
        app:layout_constraintTop_toTopOf="@+id/buttonOpenPatentesis" />

    <Button
        android:id="@+id/buttonFactorial"
        android:layout_width="0dp"
