        app:layout_constraintTop_toBottomOf="@+id/buttonEexpX" />

    <Button
        android:id="@+id/buttonPi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:onClick="setExpresion"
        android:padding="0dp"
        android:text="π"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonAbsX"
        app:layout_constraintEnd_toStartOf="@+id/buttonEuler"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/buttonAbsX"
        app:layout_constraintTop_toTopOf="@+id/buttonAbsX" />

    <Button
        android:id="@+id/buttonEuler"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:onClick="setExpresion"
        android:padding="0dp"
        android:text="ℯ"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonPi"
        app:layout_constraintEnd_toStartOf="@+id/button0"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/buttonPi"
        app:layout_constraintTop_toTopOf="@+id/buttonPi" />

    <Button
        android:id="@+id/button0"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:onClick="setExpresion"
        android:padding="0dp"
        android:text="0"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonEuler"
        app:layout_constraintEnd_toStartOf="@+id/buttonPoint"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/buttonEuler"
        app:layout_constraintTop_toTopOf="@+id/buttonEuler" />

    <Button
        android:id="@+id/buttonPoint"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:onClick="setExpresion"
        android:padding="0dp"
