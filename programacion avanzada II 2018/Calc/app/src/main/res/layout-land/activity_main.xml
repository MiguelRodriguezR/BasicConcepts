        app:layout_constraintBottom_toBottomOf="@+id/buttonLn"
        app:layout_constraintEnd_toStartOf="@+id/button1DivX"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/buttonLn"
        app:layout_constraintTop_toTopOf="@+id/buttonLn" />

    <Button
        android:id="@+id/button4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:onClick="setExpresion"
